# Copy to .env and fill in values locally or set as GitHub repo secrets.

# Docker Hub (for CI push)
DOCKERHUB_USERNAME=your_dockerhub_username
DOCKERHUB_TOKEN=your_dockerhub_access_token

# Hugging Face (if required for model downloads)
HF_TOKEN=your_huggingface_token

# Optional runtime overrides for QuickPod
HF_MODEL_ID=lodestones/Chroma
IMAGE_API_MAX_BATCH=1
